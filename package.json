{
  "name": "expo-32-playlist",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "A playlist of some songs that Greg likes to demo the Expo AV API.",
  "author": "support@expo.io",
  "private": false,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "lint": "./scripts/lint.sh && yarn build",
    "clean": "./scripts/clean.sh && yarn build",
    "read-sh": "sudo find . -name \"*.sh\" | xargs chmod u+x",
    "install-globals": "yarn global add react-native-cli typescript tslint prettier codecov",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "dev": "yarn install && yarn install-globals && yarn clean && yarn lint && yarn build && yarn test",
    "git-add": "git add -A",
    "test": "jest",
    "test-watch": "node node_modules/jest/bin/jest.js --watchAll -u",
    "test-watch-fast": "node node_modules/jest/bin/jest.js --watchAll -u --collectCoverage=false",
    "docs": "yarn generate-docs && yarn gh-pages",
    "generate-docs": "./scripts/docs.sh",
    "gh-pages": "NODE_DEBUG=gh-pages && yarn gh-clean && gh-pages --dotfiles -d docs",
    "gh-clean": "node ./node_modules/gh-pages/bin/gh-pages-clean.js",
    "preversion": "yarn lint && yarn test",
    "version": "yarn build && git add -A source",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^10.0.1",
    "@monterosa/react-native-parallax-scroll": "^1.8.0",
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "moment-timezone": "^0.5.23",
    "react-native-cross-components": "^0.2.7",
    "react-native-dotenv": "^0.2.0",
    "react-native-paper": "^2.14.0",
    "react-navigation": "^3.0.9",
    "react-navigation-material-bottom-tabs": "^1.0.0",
    "sentry-expo": "^1.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "@types/expo": "^32.0.8",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/moment-timezone": "^0.5.12",
    "@types/react": "^16.8.8",
    "babel-preset-expo": "^5.0.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.5.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.4000"
  }
}
